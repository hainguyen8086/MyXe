<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="20dp"
        android:id="@+id/cell_vehicle_imageView"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:lines="1"
        app:layout_constraintStart_toEndOf="@id/cell_vehicle_imageView"
        app:layout_constraintTop_toTopOf="@id/cell_vehicle_imageView"
        android:text="Tên xe here"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        android:id="@+id/cell_vehicle_name"
        />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:lines="1"
        app:layout_constraintStart_toEndOf="@id/cell_vehicle_imageView"
        app:layout_constraintTop_toBottomOf="@id/cell_vehicle_name"
        android:text="Tên xe here"
        android:textSize="17sp"
        android:textColor="#9F9F9F"
        android:layout_marginStart="10dp"
        android:id="@+id/cell_vehicle_id"
        />



</androidx.constraintlayout.widget.ConstraintLayout>